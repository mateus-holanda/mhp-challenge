    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.3"
